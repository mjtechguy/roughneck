# Caddy environment variables for DNS-01 challenge
# Generated by Ansible

{% if dns_provider == 'cloudflare' %}
CF_API_TOKEN={{ cloudflare_api_token }}
{% elif dns_provider == 'route53' %}
AWS_ACCESS_KEY_ID={{ route53_access_key }}
AWS_SECRET_ACCESS_KEY={{ route53_secret_key }}
{% elif dns_provider == 'digitalocean' %}
DO_AUTH_TOKEN={{ digitalocean_dns_token }}
{% elif dns_provider == 'hetzner' %}
HETZNER_API_TOKEN={{ hetzner_dns_token }}
{% endif %}
